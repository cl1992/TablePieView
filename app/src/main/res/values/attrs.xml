<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PieView">
        <attr name="pie_bg_circle_background" format="color|reference" />
        <attr name="pie_bg_circle_width" format="dimension" />
        <attr name="pie_main_circle_width" format="dimension" />
        <attr name="pie_spread_line_length_start" format="dimension" />
        <attr name="pie_spread_line_length_end" format="dimension" />
        <attr name="pie_spread_line_radius" format="dimension" />
        <attr name="pie_is_line_dash" format="boolean" />
        <attr name="pie_is_line_rounded" format="boolean" />
        <attr name="pie_is_line_circle" format="boolean" />
        <attr name="pie_spread_text" format="string" />
        <attr name="pie_spread_text_offset_horizontal" format="dimension" />
        <attr name="pie_spread_text_offset_vertical" format="dimension" />
        <attr name="pie_spread_text_size" format="dimension" />
        <attr name="pie_spread_text_direction" format="enum">
            <enum name="out" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="pie_start_angle" format="integer" />
    </declare-styleable>

    <declare-styleable name="TableView">
        <attr name="table_x_text_size" format="dimension" />
        <attr name="table_y_text_size" format="dimension" />
        <attr name="table_x_text_color" format="color|reference" />
        <attr name="table_y_text_color" format="color|reference" />
        <attr name="table_point_circle_bg_color" format="color|reference" />
        <attr name="table_point_circle_color" format="color|reference" />
        <attr name="table_point_circle_width" format="dimension" />
        <attr name="table_point_line_color" format="color|reference" />
        <attr name="table_point_line_width" format="dimension" />
        <attr name="table_area_color" format="color|reference" />
        <attr name="table_solid_line_color" format="color|reference" />
        <attr name="table_dash_line_color" format="color|reference" />
        <attr name="table_axis_line_width" format="dimension" />
        <attr name="table_point_radius" format="dimension" />
        <attr name="table_x_text_offset" format="dimension" />
        <attr name="table_y_text_offset" format="dimension" />
    </declare-styleable>


</resources>